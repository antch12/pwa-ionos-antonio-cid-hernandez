# Variables de entorno para PWA Task Manager

# Dependencias del proyecto (automáticamente instaladas)
NODE_ENV=production

# Certificados SSL en IONOS
SSL_CERT_PATH=/etc/nginx/ssl/cert.pem
SSL_KEY_PATH=/etc/nginx/ssl/key.pem

# Zona horaria
TZ=America/Mexico_City

# Configuración de Nginx (automática en docker-compose)
NGINX_PORT=80
NGINX_SSL_PORT=443

# Dominio (reemplazar con tu dominio IONOS)
DOMAIN=tu-dominio.com

# Email para Let's Encrypt
LETSENCRYPT_EMAIL=tu-email@example.com
